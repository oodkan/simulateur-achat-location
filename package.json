    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.2.3",
    "prettier": "3.0.1",
    "ts-jest": "^29.0.5",
    "typescript": "*",
    "vite": "^4.1.1"
